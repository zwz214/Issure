#ifndef __QAC_INCLUDE_cctype__
#define __QAC_INCLUDE_cctype__


// Include the original C code
#include <ctype.h>

// BEGIN BLOCK cprefix
// Some flags to control how closely the standard is followed
#include <qac_config.h>
// Deal with entities required by spec to be std namespace
__QAC_BEGIN_NAMESPACE
// END BLOCK cprefix

	using ::isalnum;
	using ::isalpha;
	using ::iscntrl;
	using ::isdigit;
	using ::isgraph;
	using ::islower;
	using ::isprint;
	using ::ispunct;
	using ::isspace;
	using ::isupper;
	using ::isxdigit;
	using ::tolower;
	using ::toupper;

// BEGIN BLOCK cpostfix
__QAC_END_NAMESPACE
// END BLOCK cpostfix

#endif // __QAC_INCLUDE_cctype__
