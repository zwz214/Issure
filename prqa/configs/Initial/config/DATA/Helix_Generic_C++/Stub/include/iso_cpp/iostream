#include <istream>
#include <ostream>

__QAC_BEGIN_NAMESPACE


  extern istream cin;
  extern ostream cout;
  extern ostream cerr;
  extern ostream clog;

  extern wistream wcin;
  extern wostream wcout;
  extern wostream wcerr;
  extern wostream wclog;


__QAC_END_NAMESPACE

